{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "maxazadf01"
		},
		"AzureSqlMI_azmxtsmi001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlMI_azmxtsmi001'"
		},
		"AzureSql_azmxtsql001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSql_azmxtsql001'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"AzureBlobStorage1_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/606ba8bb-2f0e-4ebb-87d5-d5479cbffdee/resourceGroups/2710f2c1-15a7-43e8-b222-d060ec7dd226/providers/Microsoft.Storage/storageAccounts/azadfblobstorage01"
		},
		"AzureBlobStorage1_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "maxazadf01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlMI_azmxtsmi001_connectionString": {
						"value": "[parameters('AzureSqlMI_azmxtsmi001_connectionString')]"
					},
					"AzureSql_azmxtsql001_connectionString": {
						"value": "[parameters('AzureSql_azmxtsql001_connectionString')]"
					},
					"linkedService1_connectionString": {
						"value": "[parameters('linkedService1_connectionString')]"
					},
					"AzureBlobStorage1_properties_privateLinkResourceId": {
						"value": "[parameters('AzureBlobStorage1_properties_privateLinkResourceId')]"
					},
					"AzureBlobStorage1_properties_groupId": {
						"value": "[parameters('AzureBlobStorage1_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}